			div.container
				div.row
					div.col-12.col-md-11.offset-md-1
						div.get-funded__investment-info
							h2.h2-2 Investment and Timeframe
							span.get-funded__investment-price $5,995
						div.get-funded__block-img-6
							+picture({img: 'get-funded__img-06-1200', img_320: 'get-funded__img-06-320', img_768: 'get-funded__img-06-768', img_992: 'get-funded__img-06-992', img_1200: 'get-funded__img-06-1200', alt: 'Investment and Timeframe'})
